-->
    <sql id="Base_Column_List">
        id,
        status,
        provinceId,
        name
    </sql>
    <insert id="insert" parameterType="City">
        INSERT INTO saas_city (
        status,
        provinceId,
        name
        ) VALUES (
        #{status},
        #{provinceId},
        #{name}
        )
        <selectKey resultType="java.lang.Long" keyProperty="id">
        SELECT LAST_INSERT_ID() AS ID
        </selectKey>
    </insert>

    <!-- 更新 -->
    <update id="update" parameterType="City">
        UPDATE saas_city
        <trim prefix="SET" suffixOverrides=",">
            <if test="status!=null">
                status = #{status},
            </if>
            <if test="provinceId!=null">
                provinceId = #{provinceId},
            </if>
            <if test="name!=null">
                name = #{name},
            </if>
        </trim>
        WHERE
        id = #{id}
    </update>

    <update id="updateMap">
        UPDATE saas_city
        <trim prefix="SET" suffixOverrides=",">
            <if test="map.status!=null">
                status = #{map.status},
            </if>
            <if test="map.provinceId!=null">
                provinceId = #{map.provinceId},
            </if>
            <if test="map.name!=null">
                name = #{map.name},
            </if>
        </trim>
        WHERE
        id = #{map.id}
    </update>

    <update id="updateByCondition">
        UPDATE saas_city
        <trim prefix="SET" suffixOverrides=",">
            <if test="update.status!=null">
                status = #{update.status},
            </if>
            <if test="update.provinceId!=null">
                provinceId = #{update.provinceId},
            </if>
            <if test="update.name!=null">
                name = #{update.name},
            </if>
        </trim>

        <trim prefix="WHERE" prefixOverrides="AND | OR">
            <if test="condition.id!=null">
                AND id = #{condition.id}
            </if>
            <if test="condition.status!=null">
                AND status = #{condition.status}
            </if>
            <if test="condition.provinceId!=null">
                AND provinceId = #{condition.provinceId}
            </if>
            <if test="condition.name!=null">
                AND name = #{condition.name}
            </if>
        </trim>
    </update>

    <insert id="insertMap" >
        INSERT INTO saas_city (
                status,
                provinceId,
                name
        ) VALUES (
                    #{map.status},
                    #{map.provinceId},
                    #{map.name}
        )
    </insert>


    <update id="updateNull" parameterType="City">
        UPDATE saas_city
        <trim prefix="SET" suffixOverrides=",">
            status = #{status},
            provinceId = #{provinceId},
            name = #{name},
        </trim>
        WHERE
        id = #{id} 
    </update>

    <!-- 按Id删除 -->
    <delete id="deleteById" parameterType="java.lang.Long">
        DELETE FROM saas_city
        <trim prefix="WHERE" prefixOverrides="AND | OR">
            AND id = #{id}
        </trim>
    </delete>


    <delete id="deleteByCondition" parameterType="java.util.Map">
        DELETE FROM saas_city
        <trim prefix="WHERE" prefixOverrides="AND | OR">
            <if test="id!=null">
                AND id = #{id}
            </if>
            <if test="status!=null">
                AND status = #{status}
            </if>
            <if test="provinceId!=null">
                AND provinceId = #{provinceId}
            </if>
            <if test="name!=null">
                AND name = #{name}
            </if>
        </trim>
    </delete>

    <delete id="deleteByProperty" parameterType="java.util.Map">
        DELETE FROM saas_city WHERE
        ${property} = #{value}
    </delete>

    <select id="fetch" parameterType="java.lang.Long" resultType="City">
        SELECT
        <include refid="Base_Column_List" />
        FROM saas_city
        <trim prefix="WHERE" prefixOverrides="AND | OR">
            AND id = #{id}
        </trim>
        <if test="orderBy!=null">
        ORDER BY ${orderBy} ${sortBy}
        </if>
    </select>

    <select id="findOne" parameterType="java.util.Map" resultType="City">
        SELECT
        <include refid="Base_Column_List" />
        FROM saas_city WHERE
        ${property} = #{value}
        LIMIT 0,1
    </select>

    <select id="findList" parameterType="java.util.Map" resultType="City">
        SELECT
        <include refid="Base_Column_List" />
        FROM saas_city WHERE
        ${property} = #{value}
        <if test="orderBy!=null">
            ORDER BY ${orderBy} ${sortBy}
        </if>
    </select>

    <select id="findAll" resultType="City">
        SELECT
        <include refid="Base_Column_List" />
        FROM saas_city
        <if test="orderBy!=null">
            ORDER BY ${orderBy} ${sortBy}
        </if>
    </select>

    <select id="queryPage" resultType="City">
        SELECT
        <include refid="Base_Column_List" />
        FROM saas_city
        <where>

            <if test="condition.whereSql != null">
                and id in (#{condition.whereSql})
            </if>

            <if test="condition.id!=null">
                ${condition.groupOp}   id  ${condition.id.op}  #{condition.id.data}
            </if>
            <if test="condition.status!=null">
                ${condition.groupOp}   status  ${condition.status.op}  #{condition.status.data}
            </if>
            <if test="condition.provinceId!=null">
                ${condition.groupOp}   provinceId  ${condition.provinceId.op}  #{condition.provinceId.data}
            </if>
            <if test="condition.name!=null">
                ${condition.groupOp}   name  ${condition.name.op}  #{condition.name.data}
            </if>


    </where>

        <if test="orderBy!=null">
        ORDER BY ${orderBy} ${sortBy}
        </if>
        <if test="offset != null">
            limit ${offset}, ${rows}
        </if>
    </select>

    <select id="like" parameterType="java.util.Map" resultType="City">
        SELECT
        <include refid="Base_Column_List" />
        FROM saas_city
        <trim prefix="WHERE" prefixOverrides="AND | OR">
            <if test="id!=null">
                AND id like CONCAT('%', #{id} , '%')
            </if>
            <if test="status!=null">
                AND status like CONCAT('%', #{status} , '%')
            </if>
            <if test="provinceId!=null">
                AND provinceId like CONCAT('%', #{provinceId} , '%')
            </if>
            <if test="name!=null">
                AND name like CONCAT('%', #{name} , '%')
            </if>
        </trim>
        <if test="orderBy!=null">
        ORDER BY ${orderBy} ${sortBy}
        </if>
    </select>


    <select id="queryList" parameterType="java.util.Map" resultType="City">
        SELECT
        <include refid="Base_Column_List" />
        FROM saas_city
        <trim prefix="WHERE" prefixOverrides="AND | OR">
            <if test="condition.id!=null">
                AND id = #{condition.id}
            </if>
            <if test="condition.status!=null">
                AND status = #{condition.status}
            </if>
            <if test="condition.provinceId!=null">
                AND provinceId = #{condition.provinceId}
            </if>
            <if test="condition.name!=null">
                AND name = #{condition.name}
            </if>
        </trim>
        <if test="orderBy!=null">
        ORDER BY ${orderBy} ${sortBy}
        </if>
    </select>

    <select id="queryOne" parameterType="java.util.Map" resultType="City">
        SELECT
        <include refid="Base_Column_List" />
        FROM saas_city
        <trim prefix="WHERE" prefixOverrides="AND | OR">
            <if test="condition.id!=null">
                AND id = #{condition.id}
            </if>
            <if test="condition.status!=null">
                AND status = #{condition.status}
            </if>
            <if test="condition.provinceId!=null">
                AND provinceId = #{condition.provinceId}
            </if>
            <if test="condition.name!=null">
                AND name = #{condition.name}
            </if>
        </trim>
        limit 0,1
    </select>

    <select id="count" resultType="java.lang.Integer">
        SELECT count(id) FROM saas_city
       <where>
           <if test="id!=null">
               ${groupOp} id ${id.op}  #{id.data}
           </if>
           <if test="status!=null">
               ${groupOp} status ${status.op}  #{status.data}
           </if>
           <if test="provinceId!=null">
               ${groupOp} provinceId ${provinceId.op}  #{provinceId.data}
           </if>
           <if test="name!=null">
               ${groupOp} name ${name.op}  #{name.data}
           </if>

       </where>
    <if test="orderBy!=null">
        ORDER BY ${orderBy} ${sortBy}
    </if>
    </select>

    <select id="selectMaxId" resultType="java.lang.Long">
        SELECT
        MAX(id)
        FROM saas_city
    </select>

</mapper>

